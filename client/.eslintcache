"./Components/SingleTopicPage/SingleTopicPage\";\nimport TransitionRouter from \"./Components/Router/Router\";\nimport Footer from \"./Components/Footer/Footer\";\n\nfunction App() {\n  const [state, dispatch] = Store();\n\n  useEffect(() => {\n    window.scroll(0, 0);\n    const token = window.localStorage.getItem(\"token\");\n    token &&\n      getUserProfile(token).then((user) => {\n        dispatch({ type: \"user\", payload: user });\n      });\n  }, []);\n\n  function getUserProfile(token) {\n    if (state.isAuthWithGithub) {\n      return getGithubProfile(token)\n        .then((res) => res.data)\n        .catch((error) => console.error(error));\n    } else if (state.isAuth) {\n      return getProfile(token)\n        .then((res) => res.data)\n        .catch((error) => console.error(error));\n    }\n  }\n\n  return (\n    <>\n      <React.StrictMode>\n        <ThemeProvider theme={customTheme}>\n          <Fonts />\n          <CSSReset />\n          <StateContext.Provider value={{ state, dispatch }}>\n            <NavBar />\n            <TransitionRouter>\n              <LandingPage path=\"/\" />\n              <AddTopic path=\"/add_topic\" />\n              <Profile path=\"/profile\" />\n              <SingleTopicPage path=\"single_topic/:id\" />\n              <PageNotFound default />\n            </TransitionRouter>\n            <Footer />\n          </StateContext.Provider>\n        </ThemeProvider>\n      </React.StrictMode>\n    </>\n  );\n}\n\nexport { StateContext, App };\n","/Users/andreaundecimo/Documents/Coding/Solo Project/client/src/theme/Fonts.js",[],"/Users/andreaundecimo/Documents/Coding/Solo Project/client/src/theme/index.js",[],"/Users/andreaundecimo/Documents/Coding/Solo Project/client/src/global.context/globalStore.reducer.js",[],"/Users/andreaundecimo/Documents/Coding/Solo Project/client/src/services/ApiUserClientService.js",[],"/Users/andreaundecimo/Documents/Coding/Solo Project/client/src/Components/UI_Aids/PageNotFound.jsx",[],"/Users/an