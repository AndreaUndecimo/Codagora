r\";\n\nexport default function Login() {\n  const { state, dispatch } = useContext(StateContext);\n  const [isLoading, setIsLoading] = useState(false);\n  const [userDetails, setUserDetails] = useState({ email: \"\", password: \"\" });\n  const [error, setError] = useState(\"\");\n  const [show, setShow] = useState(false);\n\n  useEffect(() => {\n    state.isAuth &&\n      !error &&\n      document.querySelector(\".form-wrapper\").classList.remove(\"show\");\n  }, [state.isAuth]);\n\n  function loginUser(credentials) {\n    return login(credentials)\n      .then((token) => {\n        getProfile(token.data).then((user) =>\n          dispatch({ type: \"user\", payload: user.data })\n        );\n        completeAuthentication(token.data);\n        return { error: null };\n      })\n      .catch((error) => ({ error: error.response.data }));\n  }\n\n  async function submitHandle(e) {\n    e.preventDefault();\n    setIsLoading(true);\n    try {\n      setError(\"\");\n      const result = await loginUser(userDetails);\n      setIsLoading(false);\n      if (!result.error) {\n        dispatch({ type: \"isAuth\", payload: true });\n      } else {\n        setError(result.error);\n      }\n      setUserDetails({ email: \"\", password: \"\" });\n    } catch (error) {\n      setError(error);\n      setIsLoading(false);\n    }\n  }\n\n  const validateForm = () => {\n    return !userDetails.email || !userDetails.password;\n  };\n\n  return (\n    <>\n      <ThemeProvider theme={customTheme}>\n        <Flex width=\"full\" align=\"center\" justifyContent=\"center\" mt={10}>\n          <Box p={2}>\n            {error && <ErrorMessage message={error} />}\n            <form onSubmit={submitHandle}>\n              <FormControl isRequired>\n                <FormLabel>Email</FormLabel>\n                <Input\n                  autoFocus={false}\n                  textOverflow=\"ellipsis\"\n                  type=\"email\"\n                  placeholder=\"Email\"\n                  onChange={(e) =>\n                    setUserDetails({ ...userDetails, email: e.target.value })\n                  }\n                  value={userDetails.email}\n                />\n              </FormControl>\n              <FormControl isRequired my={3}>\n                <FormLabel>Password</FormLabel>\n                <InputGroup size=\"md\">\n                  <Input\n                    autoFocus={false}\n                    value={userDetails.password}\n                    textOverflow=\"ellipsis\"\n                    type={show ? \"text\" : \"password\"}\n                    placeholder=\"*******\"\n                    onChange={(e) =>\n                      setUserDetails({\n                        ...userDetails,\n                        password: e.target.value,\n                      })\n                    }\n                  />\n                  <InputRightElement width=\"4.5rem\">\n                    <Button\n                      h=\"1.75rem\"\n                      size=\"sm\"\n                      onClick={() => setShow(!show)}\n                    >\n                      {show ? \"Hide\" : \"Show\"}\n                    </Button>\n                  </InputRightElement>\n                </InputGroup>\n              </FormControl>\n              <Button\n                disabled={validateForm()}\n                onClick={submitHandle}\n                width=\"full\"\n                mt={4}\n                type=\"submit\"\n                colorScheme=\"primary\"\n                variant=\"outline\"\n                boxShadow=\"sm\"\n                _hover={{ boxShadow: \"md\" }}\n                _active={{ boxShadow: \"lg\" }}\n              >\n                {isLoading ? (\n                  <CircularProgress isIndeterminate size=\"24px\" color=\"teal\" />\n                ) : (\n                  \"Sign In\"\n                )}\n              </Button>\n            </form>\n          </Box>\n        </Flex>\n      </ThemeProvider>\n    </>\n  );\n}\n","/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/ErrorMessage/ErrorMessage.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/Dashboard/Dashboard.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/services/ApiUserClientService.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/services/ApiTopicsClientService.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/TopicsGrid/TopicsGrid.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/AddTopic/AddTopic.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/global.context/globalStore.reducer.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/Subtitle.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/Profile/Profile.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/TopicCard/TopicCard.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/TopicCard/TopicCardProfile.jsx",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":28,"column":6,"nodeType":"102","endLine":28,"endColumn":8,"suggestions":"103"},{"ruleId":"100","severity":1,"message":"104","line":35,"column":6,"nodeType":"102","endLine":35,"endColumn":20,"suggestions":"105"},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["108"],"React Hook useEffect has a missing dependency: 'error'. Either include it or remove the dependency array.",["109"],"no-global-assign","no-unsafe-negation",{"desc":"110","fix":"111"},{"desc":"112","fix":"113"},"Update the dependencies array to be: [dispatch]",{"range":"114","text":"115"},"Update the dependencies array to be: [error, state.isAuth]",{"range":"116","text":"117"},[978,980],"[dispatch]",[